        botones:
        - tipoBoton: 1
          nombre: Continuar
          siguienteDialogo: 
          accion: []
          condiciones: []
      - tipoNodo: 3
        esPersonaje: 0
        personaje: 
        texto: "Cuando te encuentres con alg\xFAn habitante de la casa clica la tecla
