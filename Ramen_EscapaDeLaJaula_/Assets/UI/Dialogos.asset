        botones:
        - tipoBoton: 1
          nombre: Continuar
          siguienteDialogo: 
          accion: []
          condiciones: []
      - tipoNodo: 0
        esPersonaje: 1
        personaje: Burbuja informativa
        texto: "Por otro lado, si Ramen decide marcharse, podr\xE1 pisar hierba con
