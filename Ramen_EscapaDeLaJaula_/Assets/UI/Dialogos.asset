          siguienteDialogo: 
          accion: []
          condiciones: []
      - tipoNodo: 2
        esPersonaje: 1
        personaje: Marga La Patonisa
        texto: "Por favor, ay\xFAdame a encontrar a mis hijos y te aseguro que tendr\xE1s
